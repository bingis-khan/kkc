#$ Inner function calling nested outer functions.
#= envsize aaa 1
#= envsize f 3
#= envsize f2 1
##
## id Bool, aaa Int, println Bool
#= envsize g 3
##
## id Bool, println Bool, println Int
#= envsize a 3
# True
# 420

fn id (x)
	return x

fn a ()
	fn aaa (x)
		println(x)
		return x

	fn g ()
		fn f ()
			println(id(True))
			aaa(420)
			return

		fn f2 ()
			f()
			return

		f2()
		return

	g()
	return

a()
