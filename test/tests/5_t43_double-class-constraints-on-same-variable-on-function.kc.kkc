#$ Double class constraints on same variable on function.
# 69
# 123
# 111
# 123
# 420
# True
# 1337
# True

class Str1
	print-str (self _) -> Unit

class Str2
	print-str' (self _) -> Unit

inst Str1 Int
	print-str (self Int) -> Unit
		println(69)
		println(self)

inst Str2 Int
	print-str' (self Int) -> Unit
		println(111)
		println(self)

inst Str1 Bool
	print-str (self Bool) -> Unit
		println(420)
		println(self)

inst Str2 Bool
	print-str' (self Bool) -> Unit
		println(1337)
		println(self)

fn f (x a) -> Unit <= Str1 a, Str2 a
	print-str(x)
	print-str'(x)

f(123)
f(True)
