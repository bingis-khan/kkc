#$ Basic class constraint on function
# 69
# 123
# 420
# True

class Str
	print-str (self _) -> Unit

inst Str Int
	print-str (self Int) -> Unit
		println(69)
		println(self)

inst Str Bool
	print-str (self Bool) -> Unit
		println(420)
		println(self)

fn f (x a) -> Unit <= Str a
	print-str(x)

f(123)
f(True)
