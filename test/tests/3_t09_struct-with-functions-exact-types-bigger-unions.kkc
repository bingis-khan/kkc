CallMeBack
	KMB ((Int, Int) -> Int) (Bool -> Bool)


fn call-me-back (cmb CallMeBack)
	case cmb
		KMB(f, g)
			println(f(1, 2))
			println(f(6, 90))
			println(f(8008, 135))
			println(g(True))
			println(g(False))

	return


fn add (x, y)
	return x + y


spice = 69
fn add-with-spice (l-unspiced, r-unspiced)
	return l-unspiced + r-unspiced + spice


ignore-value = 1337
fn ignore (l, r)
	return ignore-value


hidden-bool = Just(True)
fn cucked (b)
	case hidden-bool
		None
			return b

		Just(True)
			println(3333333)
			if b
				return False
			else
				return True

		Just(False)
			println(111)
			return b

fn choose (choice, a, b, c)
	if choice == 0
		return a
	elif choice == 1
		return b
	else
		return c

get-f = fn opt: choose(opt, add, add-with-spice, ignore)

cmb0 = KMB(get-f(0), cucked)
call-me-back(cmb0)

cmb1 = KMB(get-f(1), cucked)
call-me-back(cmb1)

cmb2 = KMB(get-f(2), cucked)
call-me-back(cmb2)
