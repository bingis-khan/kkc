# 3


SomeEnum
	Ayo
	One
	Two
	More

big-type = Just(Just(Just(Two)))

fn get-enum (x)
	case x
		Just(Just(Just(e)))
			return e

		Just(Just(None))
			return Ayo

		Just(None)
			return Ayo

		None
			return Ayo

fn enum-i (e)
	case e
		Ayo
			return 0

		One
			return 1

		Two
			return 2

		More
			return 3


unpacked = get-enum(big-type)
println(enum-i(unpacked))
println(enum-i(get-enum(None)))
println(enum-i(get-enum(Just(Just(None)))))
println(enum-i(get-enum(Just(Just(Just(More))))))
