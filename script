#!./zig-out/bin/kkc
use Alloc
use List
use Slice

al = Alloc.get-c-allocator()
s = 1 to (3)
	map(Just)
	to-list(al)
	List.to-slice()

case s
	[Just(1), None, Just(3)]
		println('head is one')

	[Just(1), y, ...xs, z]
		# println(x)
		println(y)
		y <= Just(420)
		println(y)
		println(xs)
		println(z)

	xs
		println('not matched: \(xs)')

al Alloc.free(s)

