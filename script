#!./zig-out/bin/kkc
use Alloc
use List
use Slice

al = Alloc.get-c-allocator()
s = 1 to (10)
	to-list(al)
	List.to-slice()

case s
	[x, y, ...xs]
		println(x)
		println(y)
		y <= 420
		println(y)
		println(xs)

	xs
		println('not matched: \(xs)')

case 5 to (10)
	[x, y, ..., z]
		println(x)
		println(y)
		println(z)
	xs
		xs for-each(println)
		# println('not matched: \(xs)')
1 to (10) to-list(al) println()
s println()

al Alloc.free(s)
